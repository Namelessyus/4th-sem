#include <stdio.h>
#include<math.h>
int main(){
int n,i,j;

float lambda_old=1,lambda_new;
printf("enter order of matrix:");
scanf("%d",&n);
float e, x[n],x_new[n],A[n][n];
printf("enter tolerance error :");
scanf("%f",&e);
printf("enter ");
for(i=1;i<=n;i++){
for(j=1;j<=n;j++){
printf("A[%d][%d]:",i,j);
scanf("%f",&A[i][j]);

}

}
printf("enter initial guess");
for(i=1;i<=n;i++){
printf("x[%d]",i);
scanf("%f",&x[i]);
}
up:

for(i=1;i<=n;i++){
float temp=0.0;
for(j=1;j<=n;j++){
temp=temp+A[i][j]*x[j];
x_new[i]=temp;
}
}

for(i=1;i<=n;i++){
x[i]=x_new[i];

}
lambda_new=fabs(x[1]);
for(i=2;i<=n;i++){
if(fabs(x[i])>lambda_new){
lambda_new=fabs(x[i]);
}
}
for(i=1;i<=n;i++){
x[i]=x[i]/lambda_new;
}
if( fabs(lambda_new-lambda_old)>e){
lambda_old=lambda_new;
goto up;

}
printf("eigen value :%f",lambda_new);
printf("\n eigen matrix:");
for(i=1;i<=n;i++){
printf("\n x[%d]=%f",i,x[i]);
}
return 0;
}